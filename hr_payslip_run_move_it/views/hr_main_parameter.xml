<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="hr_main_parameter_form_tdoc" model="ir.ui.view">
		<field name="name">hr.main.parameter.form.tdoc</field>
		<field name="model">hr.main.parameter</field>
		<field name="inherit_id" ref="hr_base_it.hr_main_parameter_form"/>
		<field name="arch" type="xml">
			<xpath expr="//page[@name='configuration']/group[2]" position="after">
				<group string="Asiento Planillas">
					<group>
						<field name="partner_id"/>
					</group>
					<group>
						<field name="type_doc_pla"/>
					</group>
				</group>
			</xpath>
		</field>
	</record>
</odoo>