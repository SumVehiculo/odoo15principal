<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_detractions_wizard_form" model="ir.ui.view">
        <field name="name">account.detractions.wizard.form</field>
        <field name="model">account.detractions.wizard</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="fecha" required="1"/>
                    </group>
                    <group>
                        <field name="monto" required="1"/>
                    </group>
                </group>
                <footer>
                    <button string="Generar" name="generar" type="object" class="oe_highlight"/>
                    or
                    <button string="Cancel" class="oe_link" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>