<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_account_main_parameter_form_inherit_kardex" model="ir.ui.view">
		<field name="name">account.main.parameter.form.inherit</field>
		<field name="model">account.main.parameter</field>
		<field name="inherit_id" ref="account_base_it.view_account_main_parameter_form"/>
		<field name="arch" type="xml">
			<xpath expr="//notebook" position="inside">
				<page string="Kardex" name='kardex'>
					<group string="Analisis de Consumo/Costo de Ventas" name="params">
						<group>
							<field name="warehouse_ids_gs" widget="many2many_tags"/>
							<field name="location_ids_csa" widget="many2many_tags"/>
							<field name="location_dest_ids_csa" widget="many2many_tags"/>
							<field name="operation_type_ids_csa" widget="many2many_tags"/>
						</group>
					</group>
				</page>
			</xpath>
		</field>
	</record>
</odoo>