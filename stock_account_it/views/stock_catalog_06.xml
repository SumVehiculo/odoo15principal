<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="view_stock_catalog_06_form" model="ir.ui.view">
		<field name="name">stock.catalog.06.form</field>
		<field name="model">stock.catalog.06</field>
		<field name="arch" type="xml">
			<form string="Codigos de Unidad de Medida">
				<sheet>
					<group>
						<group>
							<field name="code" required="1"/>
							<field name="name" required="1"/>
						</group>
					</group>
				</sheet>
			</form>
		</field>
	</record>
	<record id="view_stock_catalog_06_list" model="ir.ui.view">
		<field name="name">stock.catalog.06.list</field>
		<field name="model">stock.catalog.06</field>
		<field name="arch" type="xml">
			<tree>
				<field name="code" optional="show"/>
				<field name="name" optional="show"/>
			</tree>
		</field>
	</record>
	<record id="action_stock_catalog_06_form" model="ir.actions.act_window">
		<field name="name">Catalogo 06 - Codigos de Unidad de Medida</field>
		<field name="res_model">stock.catalog.06</field>
		<field name="view_mode">tree,form</field>
		<field name="view_id" ref="view_stock_catalog_06_list"/>
		<field name="help" type="html">
		  <p class="o_view_nocontent_smiling_face">
			Crea tu primer registro de Codigo de Unidad de Medida aqui
		  </p>
		</field>
	</record>

	
</odoo>