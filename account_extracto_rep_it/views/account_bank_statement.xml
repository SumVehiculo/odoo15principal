<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_bank_statement_print_excel_form_inherit" model="ir.ui.view">
			<field name="name">bank.statement.print.excel.form.inherit</field>
			<field name="model">account.bank.statement</field>
			<field name="inherit_id" ref="account.view_bank_statement_form"/>
			<field name="arch" type="xml">
				<data>
					<xpath expr="//form//header" position="inside">
						<button name="generate_excel" string="Imprimir Excel" type="object"/>
					</xpath>
				</data>
			</field>
		</record>
	</data>
</odoo>